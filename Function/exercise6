/*
Crie um script que recebendo um array devolva o maior e menor valor
*/

func compareArr(arr: [Array]) -> Int {
  var min = 0
  var max_value = arr[0]
  for i in arr {
    if min < i {
      min = i
    }
    if max_value > i {
      max_value = i
    }
  }
 
   print(max_value)
}
compareArr(arr: [29,-2,10,5,11,80])
