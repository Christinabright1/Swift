
//Override
 class Dog {
   func bark(){
     print("woof")
   }
 }

class NoisyDog : Dog {
     override func bark () {
       print("woof woof woof")
     }
}
//use for in to override bark 
 class NoisyDog1 : Dog {
   override func bark () {
     for _ in 1...3 {
       super.bark()
     }
   }
 }

class VeryNoisyDog : Dog {
  override func bark () {
    for _ in 1...5 {
      super.bark()
    }
  }
}

//create instante of class
var firstDog = Dog()
print(firstDog.bark())
var secondDog = NoisyDog()
print(secondDog.bark())
var thirdDog = VeryNoisyDog()
print(thirdDog.bark())
